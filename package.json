{
  "name": "ionic-react-boilerplate",
  "version": "0.0.1",
  "private": true,
  "description": "ionic react boilerplate",
  "scripts": {
    "dev": "ionic serve -p 3000",
    "front": "ionic serve -p 3000",
    "server": "nodemon --watch server server/index.js",
    "serve:build": "npx serve -s build",
    "sync": "yarn build && npx cap sync",
    "dev:ios": "ionic capacitor run ios -l --external",
    "dev:and": "ionic capacitor run android -l --external",
    "open:and": "npx cap open android",
    "open:ios": "npx cap open ios",
    "build": "react-scripts build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "typecheck": "tsc --noEmit --incremental false",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.0",
    "@types/mobx-task": "^2.0.1",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@capacitor/core": "2.4.0",
    "@ionic/react": "^5.3.1",
    "@ionic/react-router": "^5.3.1",
    "@types/node": "^14.0.27",
    "axios": "^0.19.2",
    "ionicons": "^5.1.2",
    "mobx": "^5.15.5",
    "mobx-react-lite": "^2.0.7",
    "mobx-task": "^2.0.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "3.9.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
